<Window x:Class="ExpressTMS.DatePeriodPicker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Date Period Picker" Height="179" Width="378" ResizeMode="NoResize" Icon="/ExpressTMS;component/MainICO.ico" WindowStartupLocation="CenterOwner" Loaded="Window_Loaded">
    <Grid>
        <GroupBox Header="Period" Height="107" Name="groupBox1" VerticalAlignment="Top">
            <Grid>
                <Label Content="Start Date:" Height="28" HorizontalAlignment="Left" Margin="6,14,0,0" Name="label1" VerticalAlignment="Top" />
                <Label Content="End Date:" Height="28" HorizontalAlignment="Left" Margin="6,48,0,0" Name="label2" VerticalAlignment="Top" />
                <DatePicker Height="25" HorizontalAlignment="Left" Margin="93,17,0,0" Name="datePicker1" VerticalAlignment="Top" Width="224" />
                <DatePicker Height="25" HorizontalAlignment="Left" Margin="93,48,0,0" Name="datePicker2" VerticalAlignment="Top" Width="224" />
            </Grid>
        </GroupBox>
        <Button Content="Generate" Height="23" HorizontalAlignment="Left" Margin="99,117,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="180,117,0,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button2_Click" />
    </Grid>
</Window>
